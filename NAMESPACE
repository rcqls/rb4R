useDynLib(rb4R, .registration = TRUE)

exportPattern("^\\.rb")
exportPattern("^rb")
export(
  	is.vector.rbObj,
	print.rbObj,
	Ops.rbObj,
	Math.rbObj,	
	Summary.rbObj,
	"[.rbObj","[<-.rbObj","[[.rbObj","[[<-.rbObj","$.rbObj","$<-.rbObj",
	as.vector.rbRVector, 
	.ind4rbObj,
	as.rbRVector, 
	print.rbRVector, 
	rbObjSrv,"[.rbObjSrv","[<-.rbObjSrv",  
	.dynVar,"[.dynVarSrv","[<-.dynVarSrv","[[.dynVarSrv","[[<-.dynVarSrv",
	.dynRbVar,"[.dynRbVarSrv","[<-.dynRbVarSrv"  
)

#S3methods are not viewable inside the R system. If you want this functionality available put it in the previous export

S3method(is.vector,rbObj)
S3method(print,rbObj)
S3method(Ops,rbObj)
S3method(Math,rbObj)
S3method(Summary,rbObj)
S3method("[",rbObj)
S3method("[<-",rbObj)
S3method("[[",rbObj)
S3method("[[<-",rbObj)
S3method("$",rbObj)
S3method("$<-",rbObj)
S3method(as.vector,rbRVector)
S3method(print,rbRVector)
S3method("[",rbObjSrv)
S3method("[<-",rbObjSrv)
S3method("[",dynVarSrv)
S3method("[<-",dynVarSrv)
S3method("[[",dynVarSrv)
S3method("[[<-",dynVarSrv)
S3method("[",dynRbVarSrv)
S3method("[<-",dynRbVarSrv)
